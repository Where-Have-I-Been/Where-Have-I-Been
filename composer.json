{
    "name": "where-have-i-been/where-have-i-been",
    "type": "project",
    "description": "Where Have I Been API",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-pdo": "*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "~7.2",
        "krzysztofrewak/larahat": "^1.0",
        "laravel/framework": "^8.12",
        "laravel/sanctum": "^2.8",
        "laravel/socialite": "^5.1",
        "laravel/telescope": "^4.3",
        "laravel/tinker": "^2.5",
        "rennokki/befriended": "^3.7"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "krzysztofrewak/php-cs-double-quote-fixer": "^1.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "symplify/easy-coding-standard": "^9.0",
        "vimeo/psalm": "^4.2",
        "psalm/plugin-laravel": "^1.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "psalm": "./vendor/bin/psalm",
        "ecs": "./vendor/bin/ecs check",
        "behat": "./vendor/bin/behat --format progress",
        "check": [
            "composer psalm",
            "composer ecs",
            "composer behat"
        ]
    }
}
